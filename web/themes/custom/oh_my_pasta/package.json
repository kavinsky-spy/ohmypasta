  "scripts": {
    "prestart": "npx only-allow pnpm",
    "start": "pnpm run develop",
    "precompile": "npx only-allow pnpm",
    "compile": "pnpm run build & pnpm run build-storybook",
    "chromatic": "npx chromatic",
    "publish": "pnpm run chromatic",
    "lint": "eslint ./components",
    "a11y": "pnpm run build-storybook && ./scripts/a11y.js -r",
    "storybook": "storybook dev --ci -p 6006",
    "clean-build": "pnpm run build-storybook --no-manager-cache",
    "build-storybook": "pnpm run build && storybook build -o styleguide",
    "deploy-storybook": "storybook-to-ghpages -o styleguide",
    "webpack": "webpack --watch --config ./webpack/webpack.dev.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "predevelop": "npx only-allow pnpm",
    "develop": "concurrently --raw \"pnpm run webpack\" \"pnpm run storybook\"",
    "test": "jest --coverage",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "patch-package && node scripts/postinstall.js"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.5",
    "@drupal/once": "^1.0.1",
